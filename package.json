  },
  "bugs": {
    "url": "https://github.com/sanwebinfo/tn-cities-district/issues"
  },
  "homepage": "https://github.com/sanwebinfo/tn-cities-district",
  "devDependencies": {
    "@types/node": "^20.12.13",
    "ts-node": "^10.9.2",
